{
  "mcpServers": {
    "context7": {
      "headers": {
        "CONTEXT7_API_KEY": "${env:CONTEXT7_API_KEY}"
      },
      "url": "https://mcp.context7.com/mcp"
    },
    "fetch": {
      "args": [
        "--native-tls",
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "time": {
      "args": [
        "--native-tls",
        "mcp-server-time"
      ],
      "command": "uvx"
    }
  }
}
